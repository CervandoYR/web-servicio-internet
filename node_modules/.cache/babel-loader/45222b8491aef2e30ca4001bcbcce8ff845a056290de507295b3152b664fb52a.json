{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport L from \"leaflet\";\nimport \"leaflet/dist/leaflet.css\";\n\n// Solución para los íconos de los marcadores en Leaflet\nimport markerIconUrl from \"leaflet/dist/images/marker-icon.png\";\nimport markerShadowUrl from \"leaflet/dist/images/marker-shadow.png\";\nexport default {\n  data() {\n    return {\n      coverageAreas: [{\n        name: \"Chancay Centro\",\n        lat: -11.5625,\n        lng: -77.2708\n      }, {\n        name: \"Peralvillo\",\n        lat: -11.5723,\n        lng: -77.2655\n      }, {\n        name: \"Huaral\",\n        lat: -11.4956,\n        lng: -77.2089\n      }]\n    };\n  },\n  computed: {\n    coverageWithLinks() {\n      return this.coverageAreas.map(area => ({\n        ...area,\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\n      }));\n    }\n  },\n  mounted() {\n    // Crear el mapa centrado en Chancay\n    const map = L.map(\"map\").setView([-11.5625, -77.2708], 13);\n\n    // Agregar capa base de OpenStreetMap\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\n    }).addTo(map);\n\n    // Definir un icono de marcador personalizado\n    const customIcon = L.icon({\n      iconUrl: markerIconUrl,\n      shadowUrl: markerShadowUrl,\n      iconSize: [25, 41],\n      iconAnchor: [12, 41],\n      popupAnchor: [1, -34],\n      shadowSize: [41, 41]\n    });\n\n    // Agregar marcadores con enlaces\n    this.coverageWithLinks.forEach(area => {\n      L.marker([area.lat, area.lng], {\n        icon: customIcon\n      }).addTo(map).bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\n    });\n  }\n};","map":{"version":3,"names":["L","markerIconUrl","markerShadowUrl","data","coverageAreas","name","lat","lng","computed","coverageWithLinks","map","area","link","mounted","setView","tileLayer","attribution","addTo","customIcon","icon","iconUrl","shadowUrl","iconSize","iconAnchor","popupAnchor","shadowSize","forEach","marker","bindPopup"],"sources":["C:\\Users\\yacta\\Desktop\\S16_TG_JS ULT\\frontend\\src\\views\\CoberturaPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2 class=\"text-center my-4\">Zonas de Cobertura</h2>\r\n\r\n    <!-- Lista de lugares de cobertura -->\r\n    <div class=\"coverage-list-container\">\r\n      <ul class=\"coverage-list\">\r\n        <li v-for=\"area in coverageWithLinks\" :key=\"area.name\">\r\n          <i class=\"fas fa-map-marker-alt\"></i>\r\n          <a :href=\"area.link\" target=\"_blank\">{{ area.name }}</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <!-- Mapa de Leaflet -->\r\n    <div id=\"map\" class=\"map-container\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport L from \"leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\n\r\n// Solución para los íconos de los marcadores en Leaflet\r\nimport markerIconUrl from \"leaflet/dist/images/marker-icon.png\";\r\nimport markerShadowUrl from \"leaflet/dist/images/marker-shadow.png\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      coverageAreas: [\r\n        { name: \"Chancay Centro\", lat: -11.5625, lng: -77.2708 },\r\n        { name: \"Peralvillo\", lat: -11.5723, lng: -77.2655 },\r\n        { name: \"Huaral\", lat: -11.4956, lng: -77.2089 }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    coverageWithLinks() {\r\n      return this.coverageAreas.map(area => ({\r\n        ...area,\r\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\r\n      }));\r\n    }\r\n  },\r\n  mounted() {\r\n    // Crear el mapa centrado en Chancay\r\n    const map = L.map(\"map\").setView([-11.5625, -77.2708], 13);\r\n\r\n    // Agregar capa base de OpenStreetMap\r\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\r\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\r\n    }).addTo(map);\r\n\r\n    // Definir un icono de marcador personalizado\r\n    const customIcon = L.icon({\r\n      iconUrl: markerIconUrl,\r\n      shadowUrl: markerShadowUrl,\r\n      iconSize: [25, 41],\r\n      iconAnchor: [12, 41],\r\n      popupAnchor: [1, -34],\r\n      shadowSize: [41, 41]\r\n    });\r\n\r\n    // Agregar marcadores con enlaces\r\n    this.coverageWithLinks.forEach(area => {\r\n      L.marker([area.lat, area.lng], { icon: customIcon })\r\n        .addTo(map)\r\n        .bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\r\n    });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Estilos del mapa */\r\n.map-container {\r\n  height: 400px;\r\n  width: 100%;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Estilos de la lista */\r\n.coverage-list-container {\r\n  text-align: center;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.coverage-list {\r\n  list-style: none;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.coverage-list li {\r\n  font-size: 1.2rem;\r\n  margin: 10px 0;\r\n  color: #333;\r\n  font-weight: bold;\r\n}\r\n\r\n.coverage-list i {\r\n  color: #f1634c;\r\n  margin-right: 8px;\r\n}\r\n\r\n.coverage-list a:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";;;AAoBA,OAAOA,CAAA,MAAO,SAAS;AACvB,OAAO,0BAA0B;;AAEjC;AACA,OAAOC,aAAY,MAAO,qCAAqC;AAC/D,OAAOC,eAAc,MAAO,uCAAuC;AAEnE,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,CACb;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ,CAAC,EACxD;QAAEF,IAAI,EAAE,YAAY;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ,CAAC,EACpD;QAAEF,IAAI,EAAE,QAAQ;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ;IAEnD,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB
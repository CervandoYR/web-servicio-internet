{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport L from \"leaflet\";\nimport \"leaflet/dist/leaflet.css\";\nexport default {\n  data() {\n    return {\n      coverageAreas: [{\n        name: \"Chancay Centro\",\n        lat: -11.5625,\n        lng: -77.2708\n      }, {\n        name: \"Peralvillo\",\n        lat: -11.5723,\n        lng: -77.2655\n      }, {\n        name: \"Huaral\",\n        lat: -11.4956,\n        lng: -77.2089\n      }]\n    };\n  },\n  computed: {\n    coverageWithLinks() {\n      return this.coverageAreas.map(area => ({\n        ...area,\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\n      }));\n    }\n  },\n  mounted() {\n    const map = L.map(\"map\").setView([-11.5625, -77.2708], 12);\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\n    }).addTo(map);\n    this.coverageWithLinks.forEach(area => {\n      L.marker([area.lat, area.lng]).addTo(map).bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\n    });\n  }\n};","map":{"version":3,"names":["L","data","coverageAreas","name","lat","lng","computed","coverageWithLinks","map","area","link","mounted","setView","tileLayer","attribution","addTo","forEach","marker","bindPopup"],"sources":["C:\\Users\\yacta\\Desktop\\S16_TG_JS ULT\\frontend\\src\\views\\CoberturaPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2 class=\"text-center my-4\">Zonas de Cobertura</h2>\r\n\r\n    <!-- Mapa interactivo -->\r\n    <div id=\"map\" class=\"map-container\"></div>\r\n\r\n    <!-- Lista de lugares de cobertura -->\r\n    <div class=\"coverage-list-container\">\r\n      <ul class=\"coverage-list\">\r\n        <li v-for=\"area in coverageAreas\" :key=\"area.name\">\r\n          
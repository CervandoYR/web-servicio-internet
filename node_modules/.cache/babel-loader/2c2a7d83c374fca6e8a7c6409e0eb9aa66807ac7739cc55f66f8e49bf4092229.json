class=\"container\">\r\n    <h2 class=\"text-center my-4\">Zonas de Cobertura</h2>\r\n\r\n    <!-- Lista de lugares de cobertura -->\r\n    <div class=\"coverage-list-container\">\r\n      <ul class=\"coverage-list\">\r\n        <li v-for=\"area in coverageWithLinks\" :key=\"area.name\">\r\n          <i class=\"fas fa-map-marker-alt\"></i>\r\n          <a :href=\"area.link\" target=\"_blank\">{{ area.name }}</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <!-- Mapa de Leaflet -->\r\n    <div id=\"map\" class=\"map-container\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport L from \"leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\n\r\n// Solución para los íconos de los marcadores en Leaflet\r\nimport markerIconUrl from \"leaflet/dist/images/marker-icon.png\";\r\nimport markerShadowUrl from \"leaflet/dist/images/marker-shadow.png\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      coverageAreas: [\r\n        { name: \"Chancay Centro\", lat: -11.5625, lng: -77.2708 },\r\n        { name: \"Peralvillo\", lat: -11.5723, lng: -77.2655 },\r\n        { name: \"Huaral\", lat: -11.4956, lng: -77.2089 }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    coverageWithLinks() {\r\n      return this.coverageAreas.map(area => ({\r\n        ...area,\r\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\r\n      }));\r\n    }\r\n  },\r\n  mounted() {\r\n    // Crear el mapa centrado en Chancay\r\n    const map = L.map(\"map\").setView([-11.5625, -77.2708], 13);\r\n\r\n    // Agregar capa base de OpenStreetMap\r\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\r\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\r\n    }).addTo(map);\r\n\r\n    // Definir un icono de marcador personalizado\r\n    const customIcon = L.icon({\r\n      iconUrl: markerIconUrl,\r\n      shadowUrl: markerShadowUrl,\r\n      iconSize: [25, 41],\r\n      iconAnchor: [12, 41],\r\n      popupAnchor: [1, -34],\r\n      shadowSize: [41, 41]\r\n    });\r\n\r\n    // Agregar marcadores con enlaces\r\n    this.coverageWithLinks.forEach(area => {\r\n      L.marker([area.lat, area.lng], { icon: customIcon })\r\n        .addTo(map)\r\n        .bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\r\n    });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Estilos del mapa */\r\n.map-container {\r\n  height: 400px;\r\n  width: 100%;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Estilos de la lista */\r\n.coverage-list-container {\r\n  text-align: center;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.coverage-list {\r\n  list-style: none;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.coverage-list li {\r\n  font-size: 1.2rem;\r\n  margin: 10px 0;\r\n  color: #333;\r\n  font-weight: bold;\r\n}\r\n\r\n.coverage-list i {\r\n  color: #f1634c;\r\n  margin-right: 8px;\r\n}\r\n\r\n.coverage-list a:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EAIfA,KAAK,EAAC;AAAyB;;EAC9BA,KAAK,EAAC;AAAe;mBAN/B;;uBACEC,mBAAA,CAeM,OAfNC,UAeM,G,0BAdJC,mBAAA,CAAoD;IAAhDH,KAAK,EAAC;EAAkB,GAAC,oBAAkB,sBAE/CI,mBAAA,mCAAsC,EACtCD,mBAAA,CAOM,OAPNE,UAOM,GANJF,mBAAA,CAKK,MALLG,UAKK,I,kBAJHL,mBAAA,CAGKM,SAAA,QAVbC,WAAA,CAO2BC,QAAA,CAAAC,iBAAiB,EAAzBC,IAAI;yBAAfV,mBAAA,CAGK;MAHkCW,GAAG,EAAED,IAAI,CAACE;kCAC/CV,mBAAA,CAAqC;MAAlCH,KAAK,EAAC;IAAuB,6BAChCG,mBAAA,CAAwD;MAApDW,IAAI,EAAEH,IAAI,CAACI,IAAI;MAAEC,MAAM,EAAC;wBAAYL,IAAI,CAACE,IAAI,wBAT3DI,UAAA,E;sCAcIb,mBAAA,qBAAwB,E,0BACxBD,mBAAA,CAA0C;IAArCe,EAAE,EAAC,KAAK;IAAClB,KAAK,EAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
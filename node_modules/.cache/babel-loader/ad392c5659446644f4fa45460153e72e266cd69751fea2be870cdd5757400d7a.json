center\" id=\"beneficios\">\r\n            <div class=\"col-md-12\">\r\n                <h2 class=\"display-4 animated fadeIn text-center fw-bold \">¿CUÁLES SON LOS BENEFICIOS DE\r\n                    NUESTROS PLANES 100% FIBRA ÓPTICA?</h2>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'HomePage',\r\n    data() {\r\n    return {\r\n        benefits: [\r\n        { id: 1, title: \"Alta Velocidad\", description: \"Descarga y sube archivos en segundos.\", icon: require(\"../assets/\") },\r\n            { id: 2, title: \"Baja Latencia\", description: \"Ideal para juegos en línea y videollamadas sin cortes.\", icon: require(\"../assets/\") },\r\n            { id: 3, title: \"Red Estable\", description: \"Conexión sin interferencias y segura.\", icon: require(\"../assets/\") },\r\n            { id: 4, title: \"Soporte 24/7\", description: \"Atención rápida cuando más lo necesitas.\", icon: require(\"../assets/\") },\r\n            { id: 5, title: \"Instalación Rápida\", description: \"Te conectamos en menos de 48 horas.\", icon: require(\"../assets/installation.png\") }\r\n        ],\r\n        plans: [\r\n            { id: 1, name: \"Plan Básico\", speed: 200, price: \"S/ 50\", image: require(\"../assets/routerwifi.jpg\") },\r\n            { id: 2, name: \"Plan Avanzado\", speed: 400, price: \"S/ 79\", image: require(\"../assets/premium.jpg\") },\r\n            { id: 3, name: \"Plan Premium\", speed: 600, price: \"S/ 89\", image: require(\"../assets/gamer.webp\") }\r\n        ]\r\n    };\r\n    },\r\n    methods: {\r\n        scrollToPlans() {\r\n            document.getElementById('plans').scrollIntoView({ behavior: 'smooth' });\r\n        },\r\n        selectPlan(plan) {\r\n            alert(`Has seleccionado el ${plan.name} con ${plan.speed} Mbps por ${plan.price}`);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.animated { animation: fadeIn 1s ease-in-out; }\r\n.highlight { co
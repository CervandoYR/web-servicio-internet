{"ast":null,"code":"import ContactPage from '../views/ContactPage.vue';\nimport AOS from 'aos';\nimport 'aos/dist/aos.css';\n\n// Importar imágenes\nimport banner from '@/assets/banner.png';\nimport fibra from '@/assets/fibra.jpeg';\nimport soporte from '@/assets/soporte.jpeg';\nimport routerWifi from '@/assets/routerwifi.jpg';\nimport premium from '@/assets/premium.jpg';\nimport gamer from '@/assets/gamer.webp';\nexport default {\n  name: 'HomePage',\n  components: {\n    ContactPage\n  },\n  data() {\n    return {\n      benefits: [{\n        id: 1,\n        title: \"Velocidad Ultra Rápida\",\n        description: \"Hasta 600 Mbps simétricos para streaming, gaming y trabajo en Chancay.\",\n        image: banner\n      }, {\n        id: 2,\n        title: \"Cobertura Total en Chancay\",\n        description: \"Conexión estable en todos los barrios, desde el centro hasta zonas rurales.\",\n        image: fibra\n      }, {\n        id: 3,\n        title: \"Soporte Local 24/7\",\n        description: \"Asistencia técnica en Chancay, disponible en cualquier momento.\",\n        image: soporte\n      }],\n      plans: [{\n        id: 1,\n        name: \"INTERNET 100% FIBRA\",\n        speed: 200,\n        price: \"S/ 50\",\n        image: routerWifi,\n        link: \"https://wa.link/ce4m72\"\n      }, {\n        id: 2,\n        name: \"INTERNET 100% FIBRA\",\n        speed: 400,\n        price: \"S/ 60\",\n        image: premium,\n        link: \"https://wa.link/50xpu4\"\n      }, {\n        id: 3,\n        name: \"INTERNET 100% FIBRA\",\n        speed: 600,\n        price: \"S/ 90\",\n        image: gamer,\n        link: \"https://wa.link/zzahue\"\n      }],\n      info: [{\n        id: 1,\n        title: \"Velocidad Simétrica\",\n        description: \"Descarga y sube archivos a la misma velocidad sin interrupciones.\",\n        icon: \"bi bi-speedometer2 fa-3x text-primary\"\n      }, {\n        id: 2,\n        title: \"Mayor Conectividad\",\n        description: \"Conecta más dispositivos sin perder rendimiento.\",\n        icon: \"bi bi-wifi fa-3x text-success\"\n      }, {\n        id: 3,\n        title: \"Mayor Estabilidad\",\n        description: \"Red 100% fibra óptica, sin interferencias.\",\n        icon: \"bi bi-hdd-network fa-3x text-warning\"\n      }]\n    };\n  },\n  mounted() {\n    this.$nextTick(() => {\n      AOS.init({\n        duration: 600,\n        once: true\n      });\n\n      // Inicializar carruseles\n      ['#header-carousel', '#benefitsCarousel'].forEach(id => {\n        const el = document.querySelector(id);\n        if (el) new bootstrap.Carousel(el, {\n          interval: 3000,\n          ride: 'carousel'\n        });\n      });\n    });\n  },\n  methods: {\n    scrollToPlans() {\n      document.getElementById('plans')?.scrollIntoView({\n        behavior: 'smooth'\n      });\n    },\n    selectPlan(plan) {\n      window.open(plan.link, '_blank');\n    },\n    handleImageError(e) {\n      e.target.src = 'https://via.placeholder.com/300x200?text=Imagen+No+Disponible';\n    }\n  }\n};","map":{"version":3,"names":["ContactPage","AOS","banner","fibra","soporte","routerWifi","premium","gamer","name","components","data","benefits","id","title","description","image","plans","speed","price","link","info","icon","mounted","$nextTick","init","duration","once","forEach","el","document","querySelector","bootstrap","Carousel","interval","ride","methods","scrollToPlans","getElementById","scrollIntoView","behavior","selectPlan","plan","window","open","handleImageError","e","target","src"],"sources":["C:\\Users\\Cerva\\Desktop\\web-servicio-internet\\src\\views\\HomePage.vue"],"sourcesContent":["<template>\r\n  <!-- HERO CAROUSEL (ÚNICO) -->\r\n  <section id=\"home\" class=\"hero-carousel-container position-relative\">\r\n    <div id=\"header-carousel\" class=\"carousel slide\" data-bs-ride=\"carousel\">\r\n      <div class=\"carousel-inner\">\r\n        <div class=\"carousel-item active\" data-bs-interval=\"3000\">\r\n          <img class=\"w-100\" src=\"../assets/img/carousel-1.jpeg\" alt=\"Soluciones TI en Chancay y Perú\" loading=\"lazy\">\r\n          <div class=\"carousel-caption d-flex flex-column align-items-center justify-content-center\">\r\n            <div class=\"p-3\" style=\"max-width: 900px;\">\r\n              <h6 class=\"text-white mb-md-3\">Nos especializamos en:</h6>\r\n              <h2 class=\"text-white text-uppercase mb-md-3\">Data center y soluciones TI</h2>\r\n              <router-link to=\"/conocenos\" class=\"btn btn-primary py-md-3 px-md-5 mt-2\">Descubra más</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"carousel-item\" data-bs-interval=\"3000\">\r\n          <img class=\"w-100\" src=\"../assets/img/carousel-2.jpeg\" alt=\"Redes y Seguridad en Chancay\" loading=\"lazy\">\r\n          <div class=\"carousel-caption d-flex flex-column align-items-center justify-content-center\">\r\n            <div class=\"p-3\" style=\"max-width: 900px;\">\r\n              <h6 class=\"text-white mb-md-3\">Nos especializamos en:</h6>\r\n              <h2 class=\"text-white text-uppercase mb-md-3\">Redes y Seguridad</h2>\r\n              <router-link to=\"/conocenos\" class=\"btn btn-primary py-md-3 px-md-5 mt-2\">Descubra más</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"carousel-item\" data-bs-interval=\"3000\">\r\n          <img class=\"w-100\" src=\"../assets/img/carousel-3.jpeg\" alt=\"Seguridad Electrónica\" loading=\"lazy\">\r\n          <div class=\"carousel-caption d-flex flex-column align-items-center justify-content-center\">\r\n            <div class=\"p-3\" style=\"max-width: 900px;\">\r\n              <h6 class=\"text-white mb-md-3\">Nos especializamos en:</h6>\r\n              <h2 class=\"text-white text-uppercase mb-md-3\">Seguridad electrónica</h2>\r\n              <router-link to=\"/conocenos\" class=\"btn btn-primary py-md-3 px-md-5 mt-2\">Descubra más</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"carousel-item\" data-bs-interval=\"3000\">\r\n          <img class=\"w-100\" src=\"../assets/img/carousel-4.jpeg\" alt=\"Soluciones Tecnológicas\" loading=\"lazy\">\r\n          <div class=\"carousel-caption d-flex flex-column align-items-center justify-content-center\">\r\n            <div class=\"p-3\" style=\"max-width: 900px;\">\r\n              <h6 class=\"text-white mb-md-3\">Nos especializamos en:</h6>\r\n              <h2 class=\"text-white text-uppercase mb-md-3\">Soluciones tecnológicas</h2>\r\n              <router-link to=\"/conocenos\" class=\"btn btn-primary py-md-3 px-md-5 mt-2\">Descubra más</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <a class=\"carousel-control-prev\" href=\"#header-carousel\" role=\"button\" data-bs-slide=\"prev\">\r\n        <div class=\"btn btn-dark\" style=\"width: 45px; height: 45px;\">\r\n          <span class=\"carousel-control-prev-icon mb-n2\" aria-hidden=\"true\"></span>\r\n          <span class=\"visually-hidden\">Anterior</span>\r\n        </div>\r\n      </a>\r\n      <a class=\"carousel-control-next\" href=\"#header-carousel\" role=\"button\" data-bs-slide=\"next\">\r\n        <div class=\"btn btn-dark\" style=\"width: 45px; height: 45px;\">\r\n          <span class=\"carousel-control-next-icon mb-n2\" aria-hidden=\"true\"></span>\r\n          <span class=\"visually-hidden\">Siguiente</span>\r\n        </div>\r\n      </a>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- ABOUT -->\r\n  <section class=\"container-fluid py-4 py-md-5 about-section\">\r\n    <div class=\"container\">\r\n      <div class=\"row g-4 align-items-center\">\r\n        <div class=\"col-lg-6\" data-aos=\"fade-right\">\r\n          <div class=\"about-img position-relative overflow-hidden rounded-4\">\r\n            <img class=\"w-100 h-100 img-fluid\" src=\"../assets/img/about2.jpeg\" alt=\"Soluciones TI en Chancay\" style=\"object-fit: cover; min-height: 400px;\" loading=\"lazy\">\r\n          </div>\r\n        </div>\r\n        <div class=\"col-lg-6\" data-aos=\"fade-left\">\r\n          <div class=\"about-text bg-white p-4 p-lg-5 rounded-4 shadow-sm\">\r\n            <h6 class=\"text-primary text-uppercase mb-3 fs-6\" style=\"letter-spacing: 3px;\">Sobre Nosotros</h6>\r\n            <h2 class=\"h2 fs-3 fw-bold mb-4 text-wrap-balance\">Soluciones Tecnológicas Avanzadas en todo el Perú</h2>\r\n            <p class=\"lead fs-5 text-muted\"><strong>NetSystems</strong> es líder en soluciones tecnológicas en Chancay y todo el Perú...</p>\r\n            <p class=\"fs-6 text-muted mb-4\">Nuestros servicios incluyen:</p>\r\n            <ul class=\"fs-6 text-muted mb-4 ps-4\">\r\n              <li><strong>Instalaciones de HFC y FTTH</strong>: Redes de alta velocidad.</li>\r\n              <li><strong>Empalmes de fibra óptica</strong>: Conexiones precisas.</li>\r\n              <li><strong>Sistemas de seguridad electrónica</strong>: Cámaras y soluciones integrales.</li>\r\n              <li><strong>Data centers y cableado estructurado</strong>: Infraestructura robusta.</li>\r\n              <li><strong>Mantenimiento y soporte técnico</strong>: Atención personalizada.</li>\r\n            </ul>\r\n            <p class=\"fs-6 text-muted mb-4\">En <strong>NetSystems</strong>, nuestro compromiso es ofrecer tecnología de vanguardia...</p>\r\n            <div class=\"row g-3 mb-4\">\r\n              <div class=\"col-6\">\r\n                <img class=\"img-fluid rounded-3\" src=\"../assets/img/about-3.jpeg\" alt=\"Fibra Óptica\" loading=\"lazy\">\r\n              </div>\r\n              <div class=\"col-6\">\r\n                <img class=\"img-fluid rounded-3\" src=\"../assets/img/about-5.jpeg\" alt=\"Seguridad Electrónica\" loading=\"lazy\">\r\n              </div>\r\n            </div>\r\n            <div class=\"button-group d-flex gap-3 flex-wrap\">\r\n              <router-link to=\"/about\" class=\"btn btn-primary py-2 px-4\">Contáctanos</router-link>\r\n              <a href=\"https://w.app/gsaeeq\" class=\"btn btn-outline-primary py-2 px-4\" target=\"_blank\">Realice su cotización</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- FEATURES -->\r\n  <section class=\"container-fluid py-5 feature-section\">\r\n    <div class=\"container\">\r\n      <div class=\"row row-cols-1 row-cols-md-2 row-cols-lg-4 g-4\">\r\n        <div class=\"col\" data-aos=\"fade-up\" data-aos-delay=\"100\">\r\n          <div class=\"card h-100 shadow-sm rounded-4 border-0 transition-hover\">\r\n            <div class=\"card-body p-4 d-flex align-items-center\">\r\n              <div class=\"feature-icon flex-shrink-0 bg-primary bg-gradient text-white d-flex align-items-center justify-content-center rounded-circle me-3\" style=\"width: 60px; height: 60px;\">\r\n                <i class=\"fa fa-network-wired fa-3x\"></i>\r\n              </div>\r\n              <div class=\"feature-content\">\r\n                <h3 class=\"card-title mb-2 fs-5 fw-semibold\">TI y Redes</h3>\r\n                <p class=\"card-text text-muted mb-0\">Soluciones TI y redes de alto rendimiento.</p>\r\n                <button class=\"btn btn-primary py-2 px-4 mt-2\" @click=\"scrollToPlans\">Ver Planes</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Repite los otros 3 -->\r\n      </div>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- BENEFICIOS (CARRUSEL CORREGIDO) -->\r\n  <section class=\"benefits-section container py-5\">\r\n    <h2 class=\"section-title text-center mb-5\" data-aos=\"fade-up\">\r\n      BENEFICIOS DE NETSYSTEMS <span class=\"highlight2\">100% FIBRA ÓPTICA</span> EN <span class=\"highlight2\">CHANCAY</span>\r\n    </h2>\r\n    <div id=\"benefitsCarousel\" class=\"carousel slide mx-auto\" style=\"max-width: 600px;\" data-bs-ride=\"carousel\">\r\n      <div class=\"carousel-indicators\">\r\n        <button v-for=\"(b, i) in benefits\" :key=\"i\" :data-bs-target=\"'#benefitsCarousel'\" :data-bs-slide-to=\"i\" :class=\"{ active: i === 0 }\"></button>\r\n      </div>\r\n      <div class=\"carousel-inner\">\r\n        <div v-for=\"(b, i) in benefits\" :key=\"b.id\" class=\"carousel-item\" :class=\"{ active: i === 0 }\">\r\n          <div class=\"benefit-card mx-auto p-4 bg-white rounded-4 shadow-sm text-center\">\r\n            <img :src=\"b.image\" class=\"benefit-img img-fluid rounded-3 mb-3\" :alt=\"b.title\" @error=\"handleImageError\" loading=\"lazy\">\r\n            <h3 class=\"benefit-title fs-4 fw-bold text-primary\">{{ b.title }}</h3>\r\n            <p class=\"benefit-text text-muted\">{{ b.description }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <button class=\"carousel-control-prev\" data-bs-target=\"#benefitsCarousel\" data-bs-slide=\"prev\">\r\n        <span class=\"carousel-control-prev-icon\"></span>\r\n      </button>\r\n      <button class=\"carousel-control-next\" data-bs-target=\"#benefitsCarousel\" data-bs-slide=\"next\">\r\n        <span class=\"carousel-control-next-icon\"></span>\r\n      </button>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- PLANES -->\r\n  <section id=\"plans\" class=\"plans-section container py-5\">\r\n    <h2 class=\"section-title text-center\" data-aos=\"fade-up\">PLANES DE INTERNET WIFI NETSYSTEMS<br><span class=\"highlight2\">INSTALACIÓN GRATIS</span></h2>\r\n    <p class=\"section-subtitle text-center\" data-aos=\"fade-up\" data-aos-delay=\"100\">Elige el plan ideal para tu hogar o negocio en Chancay.</p>\r\n    <div class=\"row g-4\">\r\n      <div v-for=\"plan in plans\" :key=\"plan.id\" class=\"col-lg-4 col-md-6\" data-aos=\"fade-up\">\r\n        <div class=\"plan-card text-center p-4 bg-white rounded-4 shadow-sm\">\r\n          <h3 class=\"plan-title\">{{ plan.name }}</h3>\r\n          <p class=\"plan-speed text-primary fw-bold\">{{ plan.speed }} Mbps</p>\r\n          <img :src=\"plan.image\" class=\"plan-img img-fluid rounded-3 mb-3\" :alt=\"plan.name\" loading=\"lazy\">\r\n          <p class=\"plan-price text-danger fw-bold\">{{ plan.price }} / mes</p>\r\n          <button class=\"btn btn-cta-plan w-100\" @click=\"selectPlan(plan)\">¡Contrata Ahora!</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <p class=\"text-center availability mt-3 text-muted\" data-aos=\"fade-up\">*Verifica disponibilidad en tu zona de Chancay</p>\r\n  </section>\r\n\r\n  <!-- INFO -->\r\n  <section id=\"info\" class=\"info-section container py-5\">\r\n    <h2 class=\"section-title text-center\" data-aos=\"fade-up\">¿POR QUÉ ELEGIR NUESTRO SERVICIO DE <span class=\"highlight2\">INTERNET</span> EN CHANCAY?</h2>\r\n    <div class=\"row g-4\">\r\n      <div v-for=\"item in info\" :key=\"item.id\" class=\"col-lg-4 col-md-6\" data-aos=\"fade-up\">\r\n        <div class=\"info-card text-center p-4 bg-white rounded-4 shadow-sm\">\r\n          <i :class=\"item.icon\" class=\"info-icon text-primary mb-3\"></i>\r\n          <h5 class=\"info-title fw-semibold\">{{ item.title }}</h5>\r\n          <p class=\"info-text text-muted\">{{ item.description }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n\r\n  <!-- CONTACTO -->\r\n  <section id=\"contacto\" class=\"contact-section container py-5\">\r\n    <ContactPage />\r\n  </section>\r\n\r\n  <!-- SEO -->\r\n  <section class=\"seo-text container py-5 bg-light rounded-4\">\r\n    <h2 class=\"seo-title text-center\">Internet de Fibra Óptica en Chancay con NetSystems</h2>\r\n    <p class=\"seo-intro text-center\">Descubre el mejor <strong>servicio de internet en Chancay</strong> con NetSystems...</p>\r\n    <a href=\"#plans\" class=\"btn btn-seo-cta d-block mx-auto mt-4\">Contacta Ahora</a>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport ContactPage from '../views/ContactPage.vue';\r\nimport AOS from 'aos';\r\nimport 'aos/dist/aos.css';\r\n\r\n// Importar imágenes\r\nimport banner from '@/assets/banner.png';\r\nimport fibra from '@/assets/fibra.jpeg';\r\nimport soporte from '@/assets/soporte.jpeg';\r\nimport routerWifi from '@/assets/routerwifi.jpg';\r\nimport premium from '@/assets/premium.jpg';\r\nimport gamer from '@/assets/gamer.webp';\r\n\r\nexport default {\r\n  name: 'HomePage',\r\n  components: { ContactPage },\r\n  data() {\r\n    return {\r\n      benefits: [\r\n        { id: 1, title: \"Velocidad Ultra Rápida\", description: \"Hasta 600 Mbps simétricos para streaming, gaming y trabajo en Chancay.\", image: banner },\r\n        { id: 2, title: \"Cobertura Total en Chancay\", description: \"Conexión estable en todos los barrios, desde el centro hasta zonas rurales.\", image: fibra },\r\n        { id: 3, title: \"Soporte Local 24/7\", description: \"Asistencia técnica en Chancay, disponible en cualquier momento.\", image: soporte }\r\n      ],\r\n      plans: [\r\n        { id: 1, name: \"INTERNET 100% FIBRA\", speed: 200, price: \"S/ 50\", image: routerWifi, link: \"https://wa.link/ce4m72\" },\r\n        { id: 2, name: \"INTERNET 100% FIBRA\", speed: 400, price: \"S/ 60\", image: premium, link: \"https://wa.link/50xpu4\" },\r\n        { id: 3, name: \"INTERNET 100% FIBRA\", speed: 600, price: \"S/ 90\", image: gamer, link: \"https://wa.link/zzahue\" }\r\n      ],\r\n      info: [\r\n        { id: 1, title: \"Velocidad Simétrica\", description: \"Descarga y sube archivos a la misma velocidad sin interrupciones.\", icon: \"bi bi-speedometer2 fa-3x text-primary\" },\r\n        { id: 2, title: \"Mayor Conectividad\", description: \"Conecta más dispositivos sin perder rendimiento.\", icon: \"bi bi-wifi fa-3x text-success\" },\r\n        { id: 3, title: \"Mayor Estabilidad\", description: \"Red 100% fibra óptica, sin interferencias.\", icon: \"bi bi-hdd-network fa-3x text-warning\" }\r\n      ]\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$nextTick(() => {\r\n      AOS.init({ duration: 600, once: true });\r\n\r\n      // Inicializar carruseles\r\n      ['#header-carousel', '#benefitsCarousel'].forEach(id => {\r\n        const el = document.querySelector(id);\r\n        if (el) new bootstrap.Carousel(el, { interval: 3000, ride: 'carousel' });\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    scrollToPlans() {\r\n      document.getElementById('plans')?.scrollIntoView({ behavior: 'smooth' });\r\n    },\r\n    selectPlan(plan) {\r\n      window.open(plan.link, '_blank');\r\n    },\r\n    handleImageError(e) {\r\n      e.target.src = 'https://via.placeholder.com/300x200?text=Imagen+No+Disponible';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Fuentes */\r\n@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&display=swap');\r\n\r\n.home-page { font-family: 'Poppins', sans-serif; }\r\n\r\n.highlight { color: #ffcc00; font-weight: 700; }\r\n.highlight2 { color: #ff0000; font-weight: 700; }\r\n\r\n/* Hero Carousel */\r\n.hero-carousel-container img { height: 70vh; object-fit: cover; }\r\n.carousel-caption { background: rgba(0,0,0,0.5); }\r\n\r\n/* Botones */\r\n.btn-primary { background-color: #1D63FF; border: none; }\r\n.btn-primary:hover { background-color: #0d52d7; }\r\n\r\n.btn-cta-plan, .btn-seo-cta { background-color: #ffcc00; color: #1D63FF; }\r\n.btn-cta-plan:hover, .btn-seo-cta:hover { background-color: #e6b800; transform: translateY(-2px); }\r\n\r\n/* Responsive */\r\n@media (max-width: 768px) {\r\n  .hero-carousel-container img { height: 50vh; }\r\n  .benefit-img { height: 200px; object-fit: contain; }\r\n}\r\n</style>"],"mappings":"AAsMA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,GAAE,MAAO,KAAK;AACrB,OAAO,kBAAkB;;AAEzB;AACA,OAAOC,MAAK,MAAO,qBAAqB;AACxC,OAAOC,KAAI,MAAO,qBAAqB;AACvC,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,UAAS,MAAO,yBAAyB;AAChD,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,KAAI,MAAO,qBAAqB;AAEvC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAET;EAAY,CAAC;EAC3BU,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC
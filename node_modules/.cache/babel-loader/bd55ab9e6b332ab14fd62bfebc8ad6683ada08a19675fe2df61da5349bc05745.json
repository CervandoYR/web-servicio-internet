{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport L from \"leaflet\";\nimport \"leaflet/dist/leaflet.css\";\nimport \"leaflet-control-geocoder\"; // Para la barra de búsqueda\n\nimport markerIcon from \"leaflet/dist/images/marker-icon.png\";\nimport markerShadow from \"leaflet/dist/images/marker-shadow.png\";\nexport default {\n  data() {\n    return {\n      map: null,\n      searchQuery: \"\",\n      coverageAreas: [{\n        name: \"Chancay Centro\",\n        lat: -11.5625,\n        lng: -77.2708\n      }, {\n        name: \"Peralvillo\",\n        lat: -11.5723,\n        lng: -77.2655\n      }, {\n        name: \"Huaral\",\n        lat: -11.4956,\n        lng: -77.2089\n      }]\n    };\n  },\n  computed: {\n    coverageWithLinks() {\n      return this.coverageAreas.map(area => ({\n        ...area,\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\n      }));\n    }\n  },\n  mounted() {\n    this.map = L.map(\"map\").setView([-11.5625, -77.2708], 12);\n\n    // Capas personalizadas\n    L.tileLayer(\"https://{s}.tile.openstreetmap.fr/hot/{z}/{x}/{y}.png\", {\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\n    }).addTo(this.map);\n    const customIcon = L.icon({\n      iconUrl: markerIcon,\n      shadowUrl: markerShadow,\n      iconSize: [25, 41],\n      iconAnchor: [12, 41],\n      popupAnchor: [1, -34],\n      shadowSize: [41, 41]\n    });\n    this.coverageWithLinks.forEach(area => {\n      L.marker([area.lat, area.lng], {\n        icon: customIcon\n      }).addTo(this.map).bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\n    });\n\n    // Permitir agregar puntos manualmente\n    this.map.on(\"click\", e => {\n      L.marker([e.latlng.lat, e.latlng.lng], {\n        icon: customIcon\n      }).addTo(this.map).bindPopup(`Nuevo punto: ${e.latlng.lat.toFixed(4)}, ${e.latlng.lng.toFixed(4)}`);\n    });\n  },\n  methods: {\n    searchLocation() {\n      if (!this.searchQuery) return;\n      fetch(`https://nominatim.openstreetmap.org/search?format=json&q=${this.searchQuery}`).then(res => res.json()).then(data => {\n        if (data.length > 0) {\n          const {\n            lat,\n            lon\n          } = data[0];\n          this.map.setView([lat, lon], 14);\n          L.marker([lat, lon]).addTo(this.map).bindPopup(`Resultado de búsqueda: ${this.searchQuery}`).openPopup();\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["L","markerIcon","markerShadow","data","map","searchQuery","coverageAreas","name","lat","lng","computed","coverageWithLinks","area","link","mounted","setView","tileLayer","attribution","addTo","customIcon","icon","iconUrl","shadowUrl","iconSize","iconAnchor","popupAnchor","shadowSize","forEach","marker","bindPopup","on","e","latlng","toFixed","methods","searchLocation","fetch","then","res","json","length","lon","openPopup"],"sources":["C:\\Users\\yacta\\Desktop\\S16_TG_JS ULT\\frontend\\src\\views\\CoberturaPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2 class=\"text-center my-4\">Zonas de Cobertura</h2>\r\n\r\n    <!-- Lista de lugares de cobertura -->\r\n    <div class=\"coverage-list-container\">\r\n      <ul class=\"coverage-list\">\r\n        <li v-for=\"area in coverageAreas\" :key=\"area.name\">\r\n          <i class=\"fas fa-map-marker-alt\"></i>\r\n          <a :href=\"area.link\" target=\"_blank\">{{ area.name }}</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <!-- Barra de búsqueda -->\r\n    <input type=\"text\" v-model=\"searchQuery\" @keyup.enter=\"searchLocation\" placeholder=\"Buscar ubicación...\" class=\"search-bar\"/>\r\n    \r\n    <!-- Mapa interactivo -->\r\n    <div id=\"map\" class=\"map-container\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport L from \"leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\nimport \"leaflet-control-geocoder\"; // Para la barra de búsqueda\r\n\r\nimport markerIcon from \"leaflet/dist/images/marker-icon.png\";\r\nimport markerShadow from \"leaflet/dist/images/marker-shadow.png\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      map: null,\r\n      searchQuery: \"\",\r\n      coverageAreas: [\r\n        { name: \"Chancay Centro\", lat: -11.5625, lng: -77.2708 },\r\n        { name: \"Peralvillo\", lat: -11.5723, lng: -77.2655 },\r\n        { name: \"Huaral\", lat: -11.4956, lng: -77.2089 }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    coverageWithLinks() {\r\n      return this.coverageAreas.map(area => ({\r\n        ...area,\r\n        link: `https://www.google.com/maps?q=${area.lat},${area.lng}`\r\n      }));\r\n    }\r\n  },\r\n  mounted() {\r\n    this.map = L.map(\"map\").setView([-11.5625, -77.2708], 12);\r\n    \r\n    // Capas personalizadas\r\n    L.tileLayer(\"https://{s}.tile.openstreetmap.fr/hot/{z}/{x}/{y}.png\", {\r\n      attribution: '&copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a>'\r\n    }).addTo(this.map);\r\n    \r\n    const customIcon = L.icon({\r\n      iconUrl: markerIcon,\r\n      shadowUrl: markerShadow,\r\n      iconSize: [25, 41],\r\n      iconAnchor: [12, 41],\r\n      popupAnchor: [1, -34],\r\n      shadowSize: [41, 41]\r\n    });\r\n\r\n    this.coverageWithLinks.forEach(area => {\r\n      L.marker([area.lat, area.lng], { icon: customIcon })\r\n        .addTo(this.map)\r\n        .bindPopup(`<a href=\"${area.link}\" target=\"_blank\">${area.name}</a>`);\r\n    });\r\n\r\n    // Permitir agregar puntos manualmente\r\n    this.map.on(\"click\", (e) => {\r\n      L.marker([e.latlng.lat, e.latlng.lng], { icon: customIcon })\r\n        .addTo(this.map)\r\n        .bindPopup(`Nuevo punto: ${e.latlng.lat.toFixed(4)}, ${e.latlng.lng.toFixed(4)}`);\r\n    });\r\n  },\r\n  methods: {\r\n    searchLocation() {\r\n      if (!this.searchQuery) return;\r\n      \r\n      fetch(`https://nominatim.openstreetmap.org/search?format=json&q=${this.searchQuery}`)\r\n        .then(res => res.json())\r\n        .then(data => {\r\n          if (data.length > 0) {\r\n            const { lat, lon } = data[0];\r\n            this.map.setView([lat, lon], 14);\r\n            L.marker([lat, lon]).addTo(this.map).bindPopup(`Resultado de búsqueda: ${this.searchQuery}`).openPopup();\r\n          }\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.map-container {\r\n  height: 400px;\r\n  width: 100%;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.coverage-list-container {\r\n  text-align: center;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.coverage-list {\r\n  list-style: none;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.coverage-list li {\r\n  font-size: 1.2rem;\r\n  margin: 10px 0;\r\n  color: #333;\r\n  font-weight: bold;\r\n}\r\n\r\n.coverage-list i {\r\n  color: #f1634c;\r\n  margin-right: 8px;\r\n}\r\n\r\n.coverage-list a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.search-bar {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 8px;\r\n  margin-bottom: 15px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n</style>"],"mappings":";;;AAuBA,OAAOA,CAAA,MAAO,SAAS;AACvB,OAAO,0BAA0B;AACjC,OAAO,0BAA0B,EAAE;;AAEnC,OAAOC,UAAS,MAAO,qCAAqC;AAC5D,OAAOC,YAAW,MAAO,uCAAuC;AAEhE,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,IAAI;MACTC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,CACb;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ,CAAC,EACxD;QAAEF,IAAI,EAAE,YAAY;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ,CAAC,EACpD;QAAEF,IAAI,EAAE,QAAQ;QAAEC,GAAG,EAAE,CAAC,OAAO;QAAEC,GAAG,EAAE,CAAC;MAAQ;IAEnD,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACL,aAAa,CAACF,GAAG,CAACQ,IAAG,KAAM;QACrC,GAAGA,IAAI;QACPC,IAAI,EAAE,iCAAiCD,IA